<% provide(:title, 'New Room Type') %>

<div class="container admin_page">
  <h1>New Room Type</h1>

  <div class="row">
    <div class="col-md-4 col-md-offset-4">
      <%= form_for(@room_type) do |f| %>

        <%= render 'fields', f: f %>

        <%= f.submit "Submit Room Type", class: "btn btn-large btn-primary" %>
      <% end %>
    </div>
  </div>
</div>